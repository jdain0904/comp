#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll n;
vector <ll> dp(30);

ll f(ll num)
{
    if(dp[num]!=0) return dp[num];
    if(num==0) return dp[0]=1;
    if(num==1) return dp[1]=1;
    ll temp=0;
    if(num==2) return dp[2]=2;
    for(ll i=0; i<num; i++)
    {
        temp+=f(i)*f(num-1-i);
    }
    return dp[num]=temp;
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin>>n;
    cout<<f(n);

    return 0;
}
