/*
1-n으로 수열만들기랑 동일함
근데 이번에는 target이 정해지고 대상 문자열이 다른거임
푸는방식은 비슷
해당숫자 나올때까지 push하기--숫자 나오기전에 배열 끝나면 -1
*/
#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll n, flag;
string s;
stack <ll> stk;
vector <string> v;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    cin>>n;
    cin>>s;
    ll cur=0;
    
    for(ll target=1; target<=n; target++)
    {
        while(1){
        if(!stk.empty()&&stk.top()==target){
            stk.pop();
            v.push_back("pop");
            cur--;
            break;
        }
            if(cur>=n){
                flag=1;
                break;
            }
        char c=s[cur];
        ll num=c-48;
        if(stk.empty()||stk.top()!=target) {
           
            stk.push(num);
            v.push_back("push");
            cur++;
             
        }
       
        }   
    }

    if(flag==1) cout<<-1;
    
    else for(string s:v) cout<<s<<endl;

    return 0;
}
