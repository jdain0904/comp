#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll n, cnt;
string s;
ll dx[]={0, 1, 1, 1, 0, -1, -1, -1};
ll dy[]={-1, -1, 0, 1, 1, 1, 0, -1};
vector <vector<char>> board;

void f(pair<ll, ll> st){
    

    for(ll i=0; i<8; i++)
    {
        ll ny= st.first;
    ll nx=st.second;
    s="";
        for(ll j=0; j<4; j++)
        {
            if(nx>n||nx<=0||ny>n||ny<=0) break;

            s+=board[ny][nx];

            nx+=dx[i];
            ny+=dy[i];
        }
        if(s=="gshs") cnt++;
    }
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    cin>>n;
    board.resize(n+2, vector<char>(n+2));

    for(ll i=1; i<=n; i++){
        for(ll j=1; j<=n; j++){
            cin>>board[i][j];
        }
    }

    for(ll i=1; i<=n; i++){
        for(ll j=1; j<=n; j++){
            f({i, j});
        }
    }
    cout<<cnt;

    return 0;
}
