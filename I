#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

vector <vector<ll>> board;
pair<ll, ll> cur;
ll n, m;
ll dx[]={2, 2, 1, -1, -2, -2, 1, -1};
ll dy[]={1, -1, 2, 2, 1, -1, -2, -2};

void bfs()
{
    queue <pair<ll, ll>> q;
    board[cur.first-1][cur.second-1]=0;
    q.push({cur.first-1, cur.second-1});

    while(!q.empty())
    {
        pair <ll, ll> k=q.front();
        q.pop();



        for(ll i=0;i <8; i++)
        {
            pair <ll, ll>next={k.first+dy[i], k.second+dx[i]};

            if(next.first>=n||next.first<0||next.second>=m||next.second<0) continue;
            if(board[next.first][next.second]>board[k.first][k.second]+1)
            {
                board[next.first][next.second]=board[k.first][k.second]+1;
                q.push(next);
            }
        }
    }
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin>>n>>m;
    cin>>cur.first>>cur.second;

    board.resize(n, vector<ll>(m, INT_MAX));

    bfs();

    for(ll i=0; i<n; i++)
    {
        for(ll j=0; j<m; j++)
        {
            if(board[i][j]==INT_MAX) cout<<-1<<' ';
            else cout<<board[i][j]<<' ';
        }
        cout<<endl;
    }

    return 0;
}
