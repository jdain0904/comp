#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll dp[205][205];

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    ll n, k;
    cin>>n>>k;

    for(ll i=1; i<200; i++) dp[1][i]=1;

    for(ll i=3; i<n+1; i++)
    {
        for(ll j=2; j<k+1; j++)
        {
            for(ll a=1; a<i-1; i++)
            {
                dp[i][j]+=dp[a][j-1]*dp[i-a-1][j-1];
            } dp[i][j]%=9901;
        }
    }
   
    cout<<(9901+dp[n][k]-dp[n][k-1])%9901;


    return 0;
}
