#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll n, t;

vector <ll> v;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin>>n>>t;

   v.resize(n+2, 0);

   while(t--){
    ll a, b, c;
    cin>>a>>b>>c;
    v[a]+=c;
    v[b+1]-=c;
   }

   for(ll i=1; i<=n; i++){
    v[i]+=v[i-1];
    cout<<v[i]<<' ';
   }
    
    return 0;
}
