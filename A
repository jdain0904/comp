#include <bits/stdc++.h>
#define ll long long
#define endl "\n"

using namespace std;

ll n, q;
vector<ll> v;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin>>n>>q;
    v.resize(n+1, 0);

    for(ll i=1; i<=n; i++){
        ll temp;
        cin>>temp;
        v[i]=v[i-1]+temp;
    }

    while(q--){
        ll a, b;
        cin>>a>>b;
        cout<<v[b]-v[a-1]<<endl;
    }
    
    return 0;
}
