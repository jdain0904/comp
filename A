#include <bits/stdc++.h>

#define ll long long
#define endl "\n"

using namespace std;

ll n, m, k
ll dep;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    ll left=1, sz=0;
    cin>>n>>m>>k;
    
    while(n>0)
    {
        if(left<n) 
        {
            dep++;
            left*=2;
            n-=left
            sz=left;
        }
        
        else 
        {
            left-=n;
            break;
        }
    }
    
     while(m>0)
    {
        if(left<=m) 
        {
            m-=left;
            dep+=m/sz;
            m%=sz;
            left=sz-m;
            break;
        }
        
        else 
        {
            left-=m;
            break;
        }
    }
    
    if()
     
     cout<<dep-1;
    
    return 0;
}
